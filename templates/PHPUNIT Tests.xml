<templateSet group="PHPUNIT Tests">
  <template name="@d" value="@dataProvider" description="@dataProvider" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP Comment" value="true" />
    </context>
  </template>
  <template name="testm" value="/**&#10;* @test&#10;*/&#10;public function test$FUNCTION_NAME$() {&#10;    $END$&#10;}" description="test method" toReformat="false" toShortenFQNames="true">
    <variable name="FUNCTION_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP Class Member" value="true" />
    </context>
  </template>
  <template name="dat" value="return [&#10;    [],&#10;];&#10;" description="dataprovider return array" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>